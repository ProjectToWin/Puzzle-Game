def objtomcvformat():
	try:
		infile = input("Please input cube text file filename: ")
		objFile = open(infile, "r")
	except FileNotFoundError:
		print("File not found etc")
		infile = input("Please input cube textfile filename: ")
		objFile = open(infile, "r")

	outfile = input("Please input the new cube format filename: ")
	newFile = open(outfile, "w")

	listofgeoverts = []
	listoftextverts = []
	listofnormverts = []
	listoflistsoffaces = []

	for line in objFile:
		print(line)
		if line[0:2] == 'v ':
			a, x, y, z = line.split()
			tempTuple = (float(x), float(y), float(z))
			listofgeoverts.append(templTuple)
		elif line[0:2] == 'vt':
			a,x,y = line.split()
			tempTuple = (float(x), float(y))
			listoftextverts.append(tempTuple)

		elif line[0:2] == 'vn':
			a,x,y,z = line.split()
			tempTuple = (float(x), float(y), float(z))
			listofnormverts.append(tempTuple)

		elif line[0:2] == 'f':
		
		tempList = line.split()
		print(tempList)
		tempList = tempList[1:]
		tempL = []

		for face in tempList:
			temp = face.split('/')
			x = int(temp[0])
			y = int(temp[1])
			z = int(temp[2])
			temptuple = (x,y,z)
			tempL.append(temptuple)
			listoflistsoffaces.append(tempL)

		print('# newFile.txt', file = newFile)
		print('# MCVAN', file=newFile)
		print('# FINAL', file=newFile)
		etc.

		objFile.close()
		newFile.close()

		main():
		objtomcvformat()

		_name_ == "_main_": main()
